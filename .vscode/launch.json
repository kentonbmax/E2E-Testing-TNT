    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/node_modules/protractor/bin/protractor",
            "stopOnEntry": false,
            "args": ["${workspaceRoot}/dist/config.js"],
            "sourceMaps": true,
            "preLaunchTask": "build",
            "cwd": "${workspaceRoot}",
            "outFiles": [
                "${workspaceRoot}/dist/e2e/**/*.js"
            ]
        }
    ]
}